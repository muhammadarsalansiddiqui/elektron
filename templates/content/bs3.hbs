---
layout: false
---
<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
  <title>Elektron - Bootstrap 3</title>

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" />
  <link rel="stylesheet" href="assets/vendor/metismenu/metisMenu.min.css">
  <link rel="stylesheet" href="assets/vendor/metis-canvas/metis-canvas.min.css">
  <link rel="stylesheet" href="assets/css/elektron.css">
  <link rel="stylesheet" href="assets/css/bs3-demo.css">
</head>

<body>

  <!-- BEGIN .app -->
  <div class="app">

    <!-- BEGIN .app-wrap -->
    <div class="app-wrap">

      <!-- BEGIN .app-heading -->
      <header class="app-heading">

          <nav class="navbar navbar-default navbar-static-top">
            <div class="container-fluid">
              <!-- Brand and toggle get grouped for better mobile display -->
              <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Elektron</a>
                <button type="button" class="btn btn-primary navbar-btn" data-target="#app-side" data-toggle="canvas">MENU</button>

              </div>

              <!-- Collect the nav links, forms, and other content for toggling -->
              <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">


                <ul class="nav navbar-nav navbar-right">
                  <li><a href="#">Link</a></li>
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                      <li><a href="#">Action</a></li>
                      <li><a href="#">Another action</a></li>
                      <li><a href="#">Something else here</a></li>
                      <li role="separator" class="divider"></li>
                      <li><a href="#">Separated link</a></li>
                    </ul>
                  </li>
                </ul>
              </div><!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
          </nav>
      </header>
      <!-- END:  .app-heading -->

      <!-- BEGIN .app-container -->
      <div class="app-container">

        <!-- BEGIN .app-side -->
        <aside class="app-side canvas is-left" id="app-side">

          <!-- BEGIN .side-content -->
          <div class="side-content">
            {{> bs3-side-nav }}
          </div>
          <!-- END: .side-content -->

          <!-- BEGIN .side-footer -->
          <footer class="side-footer">
            <div class="progress">
              <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                <span class="sr-only">40% Complete (success)</span>
              </div>
            </div>
          </footer>
          <!-- END .side-footer -->

        </aside>
        <!-- END: .app-side -->

        <!-- BEGIN .app-main -->
        <div class="app-main">

          <!-- BEGIN .main-heading -->
          <header class="main-heading">
            <ol class="breadcrumb">
              <li><a href="index.html">Elektron</a></li>
              <li class="active">Bootstrap 3</li>
            </ol>
          </header>
          <!-- END: .main-heading -->

          <!-- BEGIN .main-content -->
          <div class="main-content">
            <div class="alert alert-info" role="alert">
              <a href="http://getbootstrap.com" target="_blank"  class="alert-link">Download Bootstrap 3</a>
            </div>

              <div class="container-fluid">
                <div class="row">

                  <div class="col-md-4">
                    <div class="panel panel-default">
                      <div class="panel-heading">
                        <h3 class="panel-title">Login</h3>
                      </div>
                      <div class="panel-body">
                        <div class="form-group">
                          <label for="username">Username <span class="text-danger">*</span> </label>
                          <input type="text" class="form-control" id="username" placeholder="username">
                        </div>
                        <div class="form-group">
                          <label for="password">Password <span class="text-danger">*</span> </label>
                          <input type="text" class="form-control" id="password" placeholder="password">
                        </div>
                        <div class="checkbox">
                          <label>
                            <input type="checkbox"> Remember Me
                          </label>
                        </div>
                        <div class="text-right">
                          <button type="submit" class="btn btn-primary">Submit</button>
                        </div>

                      </div>
                    </div>
                  </div>

                  <div class="col-md-4">
                    <div class="panel panel-primary">
                      <div class="panel-heading">
                        <h3 class="panel-title">Panel Title</h3>
                      </div>
                      <div class="panel-body">
                        Basic panel example
                      </div>
                      <footer class="panel-footer">
                        Panel footer
                      </footer>
                    </div>
                  </div>

                  <div class="col-md-4">
                    <div class="panel panel-success">
                      <div class="panel-heading">Panel heading</div>
                      <div class="panel-body">
                        <p>Some default panel content here. Nulla vitae elit libero,
                          a pharetra augue. Aenean lacinia bibendum nulla sed consectetur.
                          Aenean eu leo quam. Pellentesque ornare sem lacinia quam venenatis
                          vestibulum. Nullam id dolor id nibh ultricies vehicula ut id elit.</p>
                        </div>
                        <div class="table-responsive">
                          <table class="table table-condensed  table-hover table-bordered table-striped">
                            <thead>
                              <tr>
                                <th>#</th>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Username</th>
                              </tr>
                            </thead>
                            <tbody>
                              <tr class="danger"> <th scope="row">1</th> <td>Mark</td> <td>Otto</td> <td>@mdo</td> </tr>
                              <tr class="info"> <th scope="row">2</th> <td>Jacob</td> <td>Thornton</td> <td>@fat</td> </tr>
                              <tr class="warning"> <th scope="row">3</th> <td>Larry</td> <td>the Bird</td> <td>@twitter</td> </tr>
                            </tbody>
                          </table>
                        </div>
                      </div>
                  </div>

                </div>
              </div>

          </div>
          <!-- END: .main-content -->

          <!-- BEGIN .main-footer -->
          <footer class="main-footer">
            main footer
          </footer>
          <!-- END: .main-footer -->

        </div>
        <!-- END: .app-main -->

      </div>
      <!-- END: .app-container -->

      <!-- begin .app-footer -->
      <footer class="app-footer">
        <div class="container-fluid">
          <p class="text-center">
            {{pkg.name}} v{{pkg.version}} 2017
          </p>
        </div>
      </footer>
      <!-- END: .app-footer -->

    </div>
    <!-- END: .app-wrap -->

  </div>
  <!-- END: .app -->

  <!-- jQuery first, then Tether, then other JS. -->
  <script src="assets/vendor/jquery/jquery.min.js" charset="utf-8"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script src="assets/vendor/metismenu/metisMenu.min.js" charset="utf-8"></script>
  <script src="assets/vendor/metis-canvas/metis-canvas.min.js" charset="utf-8"></script>

  <script src="assets/js/elektron.js" charset="utf-8"></script>

</body>

</html>

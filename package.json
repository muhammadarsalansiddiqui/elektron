{
  "name": "elektron",
  "version": "0.1.0",
  "description": "An Admin Toolkit",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "start": "assemble serve",
    "scss": "assemble scss",
    "js": "assemble js",
    "content": "assemble",
    "assets": "assemble --file assets.js",
    "build": "npm run assets & npm run content"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onokumus/elektron.git"
  },
  "keywords": [
    "admin",
    "toolkit",
    "elektron",
    "jquery"
  ],
  "author": "Osman Nuri Okumus <onokumus@gmail.com> (https://github.com/onokumus)",
  "contributors": [
    "Osman Nuri Okumus <onokumus@gmail.com> (https://github.com/onokumus)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onokumus/elektron/issues"
  },
  "homepage": "https://github.com/onokumus/elektron#readme",
  "files": [
    "index.js",
    "assets.js",
    "assemblefile.js",
    "templates",
    "js",
    "scss",
    "LICENSE"
  ],
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "assemble": "^0.23.0",
    "auto": "^0.1.2",
    "babel-preset-env": "^1.1.8",
    "base-watch": "^0.1.3",
    "browser-sync": "^2.18.8",
    "gulp-babel": "^6.1.2",
    "gulp-extname": "^0.2.2",
    "gulp-prettify": "^0.5.0",
    "gulp-sass": "^3.1.0",
    "handlebars-helpers": "^0.8.0",
    "helper-markdown": "^0.2.1",
    "helper-md": "^0.2.1"
  },
  "dependencies": {
    "metis-canvas": "^1.0.1",
    "metismenu": "^2.6.3"
  },
  "verb": {
    "toc": {
      "collapsible": true,
      "method": "preWrite"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "helpers": [
      "helper-changelog"
    ],
    "related": {
      "list": [
        "metismenu",
        "twbuttons",
        "label2srt"
      ]
    },
    "reflinks": [
      "metismenu",
      "label2srt",
      "twbuttons",
      "helper-changelog",
      "keep-a-changelog"
    ]
  }
}

{
  "name": "elektron",
  "version": "0.3.3",
  "description": "An Admin Layout",
  "main": "index.js",
  "browser": "dist/elektron.min.js",
  "unpkg": "dist/elektron.min.js",
  "jsdelivr": "dist/elektron.min.js",
  "style": "dist/elektron.css",
  "sass": "scss/elektron.scss",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "start": "gulp serve",
    "css": "gulp css",
    "js": "gulp js",
    "pages": "gulp pages",
    "assets": "gulp assets",
    "build": "npm run assets & npm run pages",
    "test": "eslint js",
    "precommit": "npm test",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onokumus/elektron.git"
  },
  "keywords": [
    "admin",
    "layout",
    "elektron",
    "jquery"
  ],
  "author": "Osman Nuri Okumus <onokumus@gmail.com> (https://github.com/onokumus)",
  "contributors": [
    "Osman Nuri Okumus <onokumus@gmail.com> (https://github.com/onokumus)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onokumus/elektron/issues"
  },
  "homepage": "https://github.com/onokumus/elektron#readme",
  "files": [
    "index.js",
    "assets.js",
    "assemblefile.js",
    "templates/",
    "dist/",
    "js/",
    "scss/",
    "README.md"
  ],
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "auto": "^0.1.2",
    "autoprefixer": "^8.3.0",
    "babel-core": "6",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-extname": "^0.2.2",
    "gulp-header": "^2.0.5",
    "gulp-postcss": "^7.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-uglify": "^3.0.0",
    "handlebars-helpers": "^0.10.0",
    "helper-markdown": "^1.0.0",
    "helper-md": "^0.2.2",
    "husky": "^0.14.3",
    "open-color": "^1.6.3",
    "postcss-flexbugs-fixes": "^3.3.0",
    "rtlcss": "^2.2.1"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "metismenu": "^2.7.0",
    "onoffcanvas": "^1.1.2"
  },
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "helpers": [
      "helper-changelog"
    ],
    "related": {
      "list": [
        "metismenu",
        "twbuttons",
        "label2srt"
      ]
    },
    "reflinks": [
      "metismenu",
      "label2srt",
      "twbuttons",
      "helper-changelog",
      "keep-a-changelog"
    ]
  }
